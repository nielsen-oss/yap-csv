{
  "name": "@nielsen-oss/yap-csv",
  "version": "1.0.0",
  "description": "Node.js package that manages CSV to JSON conversion supporting complex field mapping",
  "repository": {
    "type": "git",
    "url": "git@github.com:nielsen-oss/yap-csv.git"
  },
  "main": "index.js",
  "scripts": {
    "doc": "jsdoc . -c ./jsdoc.json",
    "lint": "standard",
    "lintFix": "standard --fix",
    "test:unit": "nyc ava '**/test/unit/*.test.js' "
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-eslint": "^6.0.4",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-tape-runner": "^2.0.1",
    "jsdoc": "^3.4.2",
    "nyc": "^7.1.0",
    "proxyquire": "^1.7.10",
    "sinon": "^2.0.0-pre.4",
    "standard": "^7.1.0",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.2",
    "tap-nyan": "0.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "bluebird": "^3.3.3",
    "body-parser": "^1.15.0",
    "csv": "^1.1.0",
    "express": "^4.13.4",
    "express-validation": "^0.6.0",
    "express-winston": "^1.2.0",
    "express-winston-middleware": "^0.0.6",
    "lodash": "^4.17.2",
    "uuid": "^3.0.1"
  },
  "license": "Apache-2.0",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "syntax-async-functions",
      "transform-regenerator"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "nyc": {
    "statements": 50,
    "branches": 50,
    "functions": 50,
    "lines": 50,
    "reporter": [
      "text",
      "lcov",
      "text-summary"
    ],
    "check-coverage": true,
    "exclude": [
      "**/*.spec.js",
      "**/test/**"
    ]
  }
}
